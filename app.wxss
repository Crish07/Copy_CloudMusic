/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 250rpx 0 200rpx;
  box-sizing: border-box;
} 

/* 搜索及音乐列表 */
.listTd {
  padding: 10px 30px 10px 10px; 
  display: flex;
  justify-content: space-between;
  align-items: center;
  font-size: 16px;
  border-bottom: 1px solid #f0f0f0;
}
.listTd>image {
  min-width: 50rpx;
  max-width: 50rpx;
  height: 50rpx;
}
.listTd-left {
  display: flex;
  align-items: center;
}
.listTd-left>text {
  display: inline-block;
  width: 70rpx;
}
.listTd-SQ {
  margin-top: 5px;
  display: flex;
  align-items: center;
}
.listTd-SQ image {
  width: 27rpx;
  height: 17rpx;
  margin-right: 5px;
}
.listTd-SQ text {
  font-size: 12px;
  color: #555555;
}